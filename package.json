  "bugs": {
    "url": "https://github.com/neplextech/canvacord/issues"
  },
  "homepage": "https://github.com/neplextech/canvacord#readme",
  "packageManager": "yarn@3.6.0",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/react": "^18.2.12",
    "benny": "^3.7.1",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.3",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.3"
  }
}
