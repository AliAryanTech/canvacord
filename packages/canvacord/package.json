{
    "name": "canvacord",
    "description": "Generate images on-the-fly with the help of wide range of templates.",
    "version": "6.0.0-beta.3",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "module": "./dist/index.mjs",
    "files": [
        "dist"
    ],
    "scripts": {
        "format": "yarn dlx @biomejs/biome format src --write",
        "build": "tsup",
        "test": "tsx ./test/index.ts",
        "test:lb": "tsx ./test/leaderboard.ts",
        "test:jsx": "tsx ./test/jsxTest.tsx",
        "test:demo": "tsx ./test/demo.tsx",
        "bench": "cd ./benchmark && node jsx-renderer.mjs",
        "lint": "yarn dlx @biomejs/biome lint src"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/neplextech/canvacord.git"
    },
    "keywords": [
        "canavcord",
        "image",
        "api",
        "manipulation",
        "nodejs",
        "typescript"
    ],
    "author": "twlite",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/neplextech/canvacord/issues"
    },
    "homepage": "https://canvacord.js.org",
    "packageManager": "yarn@3.6.0",
    "devDependencies": {
        "@biomejs/biome": "^1.4.1",
        "@types/node": "^20.3.1",
        "@types/react": "^18.2.12",
        "benny": "^3.7.1",
        "biome": "workspace:^",
        "tailwindcss": "^3.3.3",
        "tsconfig": "workspace:^",
        "tsup": "^7.2.0",
        "tsx": "^3.12.7",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@napi-rs/canvas": "^0.1.44",
        "@napi-rs/image": "^1.7.0",
        "@resvg/resvg-js": "^2.6.0",
        "@skyra/gifenc": "^1.0.1",
        "file-type": "16.5.4",
        "satori": "^0.10.11",
        "tailwind-merge": "^2.0.0"
    }
}